const{Command:Command}=require("commands"),{log:log}=require("api"),permissions=require("permissions"),{performAllPlayers:performAllPlayers}=require("players");exports.me=((e,s,m)=>{if(new permissions.Permission("mcjs.command.me").hasPermission(s.username)){const m={text:`* ${s.username} ${e.join(" ")}`};performAllPlayers(e=>{s.sendMessage(m,e)}),log("chat",m.text)}else permissions.defaultError(s._client)}),exports.init=(()=>{new Command({command:"me",filename:"me.mcjsp",info:"Sends a message about yourself in chat.",usage:"/me <message>"})});