const{Command:Command}=require("commands"),permissions=require("permissions"),{TickAction:TickAction}=require("tick");var tps=0,ticks=0;exports.tps=((s,e,i)=>{new permissions.Permission("mcjs.command.tps").hasPermission(e.username)?e.sendMessage([{text:"The current TPS is: ",color:"aqua"},{text:tps,color:"aqua",bold:!0}]):permissions.defaultError(e._client)}),exports.init=(()=>{new Command({command:"tps",filename:"tps.mcjsp",info:"Shows the current ticks/second",usage:"/tps"}),new TickAction("/tps",()=>{ticks++}),setInterval(()=>{tps=ticks,ticks=0},1e3)});