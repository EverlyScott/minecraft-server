const{Command:Command}=require("commands"),permissions=require("permissions");exports.ping=((s,n,e)=>{new permissions.Permission("mcjs.command.ping").hasPermission(n.username)?n.sendMessage([{text:"Your ping is ",color:"aqua"},{text:n.latency,color:"aqua",bold:!0},{text:"ms",color:"aqua",bold:!1}]):permissions.defaultError(n._client)}),exports.init=(()=>{new Command({command:"ping",filename:"ping.mcjsp",info:"Shows your current ping in milliseconds",usage:"/ping"})});